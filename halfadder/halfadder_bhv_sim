#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5acfe383aa70 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5acfe384e470_0 .var "A", 0 0;
v0x5acfe384e530_0 .var "B", 0 0;
v0x5acfe384e600_0 .net "Cout", 0 0, v0x5acfe384e230_0;  1 drivers
v0x5acfe384e700_0 .net "S", 0 0, v0x5acfe384e300_0;  1 drivers
S_0x5acfe383ac00 .scope module, "DUT" "halfadder_bhv" 2 4, 3 1 0, S_0x5acfe383aa70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "S";
    .port_info 3 /OUTPUT 1 "Cout";
v0x5acfe3801490_0 .net "A", 0 0, v0x5acfe384e470_0;  1 drivers
v0x5acfe384e170_0 .net "B", 0 0, v0x5acfe384e530_0;  1 drivers
v0x5acfe384e230_0 .var "Cout", 0 0;
v0x5acfe384e300_0 .var "S", 0 0;
E_0x5acfe3801c30 .event anyedge, v0x5acfe3801490_0, v0x5acfe384e170_0;
    .scope S_0x5acfe383ac00;
T_0 ;
    %wait E_0x5acfe3801c30;
    %load/vec4 v0x5acfe3801490_0;
    %load/vec4 v0x5acfe384e170_0;
    %xor;
    %store/vec4 v0x5acfe384e300_0, 0, 1;
    %load/vec4 v0x5acfe3801490_0;
    %load/vec4 v0x5acfe384e170_0;
    %and;
    %store/vec4 v0x5acfe384e230_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5acfe383aa70;
T_1 ;
    %vpi_call 2 6 "$dumpfile", "halfadder_behavioral.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5acfe383aa70 {0 0 0};
    %vpi_call 2 8 "$monitor", $time, " A=%b, B=%b,  S=%b, Cout=%b", v0x5acfe384e470_0, v0x5acfe384e530_0, v0x5acfe384e700_0, v0x5acfe384e600_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5acfe384e470_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5acfe384e530_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5acfe384e470_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5acfe384e530_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5acfe384e470_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5acfe384e530_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5acfe384e470_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5acfe384e530_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "halfadder_behavioural_tb.v";
    "halfadder_behavioural.v";
