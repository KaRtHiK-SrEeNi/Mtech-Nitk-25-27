v0x594541e38950_0 .net "Cout", 0 0, L_0x594541e39050;  alias, 1 drivers
v0x594541e38a20_0 .net "Sum", 0 0, L_0x594541e38ef0;  alias, 1 drivers
    .scope S_0x594541e25810;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "halfadder_strl.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x594541e25810 {0 0 0};
    %vpi_call 2 11 "$monitor", $time, " A=%b  B=%b  Sum=%b  Cout=%b", v0x594541e38b90_0, v0x594541e38c50_0, v0x594541e38e20_0, v0x594541e38d20_0 {0 0 0};
