    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594541e38b90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594541e38c50_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594541e38b90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594541e38c50_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594541e38b90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594541e38c50_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594541e38b90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594541e38c50_0, 0, 1;
