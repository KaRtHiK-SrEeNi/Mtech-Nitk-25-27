    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "Cout";
    .port_info 3 /OUTPUT 1 "Sum";
L_0x594541e38ef0 .functor XOR 1, v0x594541e38b90_0, v0x594541e38c50_0, C4<0>, C4<0>;
